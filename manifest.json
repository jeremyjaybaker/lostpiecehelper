{
    "name": "Rebrickable Lost Part Helper",
    "description": "Shows a list of lost parts on each set page",
    "version": "0.1",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "/images/stud16.png",
        "32": "/images/stud32.png",
        "48": "/images/stud48.png",
        "128": "/images/stud128.png"
      }
    },
    "icons": {
      "16": "/images/stud16.png",
      "32": "/images/stud32.png",
      "48": "/images/stud48.png",
      "128": "/images/stud128.png"
    }
  }